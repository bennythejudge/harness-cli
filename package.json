{
  "name": "harness-cli",
  "version": "0.0.0",
  "author": "Luke Hertert @ldhertert",
  "bin": {
    "harness": "./bin/run"
  },
  "bugs": "https://github.com/ldhertert/harness-automation/issues",
  "dependencies": {
    "@oclif/command": "*",
    "@oclif/config": "*",
    "@oclif/plugin-help": "*",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "*",
    "@oclif/test": "*",
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-config-oclif": "*",
    "eslint-config-oclif-typescript": "*",
    "globby": "*",
    "mocha": "*",
    "nyc": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ldhertert/harness-automation",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "harness",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ldhertert/harness-automation",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
